{
  "version": 2,
  "builds": [
    {
      "src": "next.config.js",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "https://viafarm-1.onrender.com"
  },
  "outputDirectory": ".next",
  "framework": "nextjs",
  "regions": ["bom1"]
}
